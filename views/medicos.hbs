<div class="logoHospital">
    <img src="/images/logo hospital.png" alt="" width="99px">
    <h1>Centro m√©dico</h1>
</div>
<h2>LISTADO DE MEDICOS</h2>
<table border="1">
    <thead>
        <tr>
            <th>Cedula</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Especialidad</th>
            <th>Consultorio</th>
            <th>Correo</th>
            <th>Opciones</th>
        </tr>
    </thead>
    <tbody>
        {{#each medicos}}
        <tr class="celdaMedicos">
            <td>{{cedula}}</td>
            <td>{{nombre}}</td>
            <td>{{apellido}}</td>
            <td>{{especialidad}}</td>
            <td>{{consultorio}}</td>
            <td>{{correo}}</td>
            <td><a href="/medicos/eliminar/{{cedula}}" >Eliminar</a></td>
        </tr>
        {{/each}}
    </tbody>
</table>
<div class="botPac">
    <a class="volver" href="/">Volver</a>
    <a href="medicos/agregar-medico">Agregar medico</a>
</div>